{
    "$schema": "https://railway.com/railway.schema.json",
    "services": [
        {
        "name": "celery-worker",
        "startCommand": "celery -A celery_app.celery worker --loglevel=info --max-tasks-per-child 100 --concurrency=2"
        },
        {
            "name": "celery-beat",
            "startCommand": "celery -A celery_app.celery beat -l info --scheduler celery_sqlalchemy_scheduler.schedulers.DatabaseScheduler"
        }
    ]
}
